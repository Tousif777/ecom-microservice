# User Service Environment Variables
NODE_ENV=development
PORT=3001

# Database Configuration
DATABASE_URL="postgresql://admin:admin123@postgres:5432/myapp?schema=user_service"
# For local development: DATABASE_URL="postgresql://admin:admin123@localhost:5432/myapp?schema=user_service"

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-user-service
JWT_EXPIRES_IN=24h

# Redis Configuration
REDIS_URL=redis://redis:6379
# For local development: REDIS_URL=redis://localhost:6379

# RabbitMQ Configuration
RABBITMQ_URL=amqp://admin:admin123@rabbitmq:5672
# For local development: RABBITMQ_URL=amqp://admin:admin123@localhost:5672

# Password Hashing
BCRYPT_ROUNDS=12
